{
  "name": "@meemoo/admin-core-ui",
  "version": "2.0.10",
  "private": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "preinstall": "npx force-resolutions",
    "prepare----disabled-since-it-is-throwing-errors": "husky install",
    "start": "cross-env PORT=3400 dotenv -e .env/.env.local react-app-rewired start",
    "prebuild": "rimraf dist",
    "build": "mv src/index-export.ts src/index.ts && rollup -c && mv src/index.ts src/index-export.ts",
    "build-and-copy-to-client-hetarchief": "rimraf ../../hetarchief-client/node_modules/@meemoo/admin-core-ui/dist && npm run build && cp -r dist package.json ../../hetarchief-client/node_modules/@meemoo/admin-core-ui",
    "build-and-copy-to-client-avo": "rimraf ../../avo2-client/node_modules/@meemoo/admin-core-ui/dist ../../avo2-client/node_modules/.cache && npm run build && cp -r dist package.json ../../avo2-client/node_modules/@meemoo/admin-core-ui",
    "build:analyze": "npm run build -- --config-analyze",
    "format": "npm run lint:ts -- --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint --allow-empty-input src/**/*.{css,scss}",
    "lint:scss:fix": "stylelint --fix --allow-empty-input src/**/*.{css,scss}",
    "test": "jest --cache",
    "type-check": "tsc --noEmit",
    "whitelist-queries": "ts-node --project ./scripts/tsconfig.json -- scripts/whitelist-gql-queries.ts",
    "i18n:extract": "dotenv -e .env/.env.local ts-node --transpile-only --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.ts && npm run lint:ts -- --fix",
    "convert-gql-queries-to-separate-files": "ts-node --transpile-only --require dotenv/config --project ./scripts/tsconfig.json -- scripts/convert-gql-queries-to-separate-graphql-files.ts",
    "generate-database-types": "npm run generate-hetarchief-database-types && npm run generate-avo-database-types",
    "generate-hetarchief-database-types": "dotenv -e .env/.env.local graphql-codegen -- --config codegen-hetarchief.yml",
    "generate-avo-database-types": "dotenv -e .env/.env.local graphql-codegen -- --config codegen-avo.yml"
  },
  "peerDependencies": {
    "@meemoo/react-components": "2.21.5",
    "@tanstack/react-query": "^4.14.1",
    "@viaa/avo2-components": "3.1.1",

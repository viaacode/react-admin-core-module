{
  "name": "@meemoo/admin-core-ui",
  "version": "4.5.28",
  "private": false,
  "type": "module",
  "exports": {
    "./dist/admin.mjs": {
      "types": "./dist/admin.d.mts",
      "import": "./dist/admin.mjs",
      "default": "./dist/admin.mjs"
    },
    "./dist/client.mjs": {
      "types": "./dist/client.d.mts",
      "import": "./dist/client.mjs",
      "default": "./dist/client.mjs"
    }
  },
  "scripts": {
    "prepare": "cd .. && husky install",
    "dev": "cross-env PORT=3400 dotenv -e .env/.env.local vite",
    "prebuild": "rimraf dist",
    "build": "vite build",
    "serve": "vite preview",
    "copy-to-client-hetarchief": "rimraf ../../hetarchief-client/node_modules/@meemoo/admin-core-ui/dist ../../hetarchief-client/node_modules/.cache ../../hetarchief-client/.swc ../../hetarchief-client/.next && rimraf ../../hetarchief-client/.next && cp -r dist package.json ../../hetarchief-client/node_modules/@meemoo/admin-core-ui",
    "copy-to-client-avo": "rimraf ../../avo2-client/node_modules/@meemoo/admin-core-ui/dist ../../avo2-client/node_modules/.cache  ../../avo2-client/node_modules/.vite && cp -r dist package.json ../../avo2-client/node_modules/@meemoo/admin-core-ui",
    "copy": "vite build && npm run copy-to-client-avo && npm run copy-to-client-hetarchief",
    "copy:avo": "vite build && npm run copy-to-client-avo",
    "build:analyze": "npx vite-bundle-visualizer",
    "format": "biome format --write src",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "biome lint --write .",
    "lint:scss": "stylelint --fix src/**/*.{css,scss}",
    "test": "jest --cache",
    "type-check": "tsc --noEmit",
    "backup-translations-hetarchief-qas": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/backup-translations.mts -- HET_ARCHIEF",
    "backup-translations-avo-qas": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/backup-translations.mts -- AVO",
    "translations:extract:hetarchief": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.mts -- HET_ARCHIEF",
    "translations:extract:avo": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.mts -- AVO",
    "list-of-changed-translations-hetarchief": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/get-list-of-changed-translations.mts -- HET_ARCHIEF v4.5.20 v4.5.23",
    "list-commit-messages-between-tags-hetarchief": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/get-list-of-commit-messages-between-tags.mts -- v4.5.20 v4.5.23",
    "list-of-changed-translations-avo": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/get-list-of-changed-translations.mts -- AVO v4.5.17 v4.5.18",
    "list-commit-messages-between-tags-avo": "dotenv -e .env/.env.local tsx --project ./scripts/tsconfig.json scripts/get-list-of-commit-messages-between-tags.mts -- v4.5.17 v4.5.18",
    "check-bundled-files": "npx depcruise src/react-admin --config bundle-import-rules.json --include-only \"^src\" --exclude \"(.spec.tsx?|.test.tsx?|node_modules)\"",
    "check-bundled-files:admin.mts": "npx depcruise src/admin.mts --config bundle-import-rules.json --include-only \"^src\" --exclude \"(.spec.tsx?|.test.tsx?|node_modules)\"",
    "check-bundled-files:client.mts": "npx depcruise src/client.mts --config bundle-import-rules.json --include-only \"^src\" --exclude \"(.spec.tsx?|.test.tsx?|node_modules)\" --output-type dot | dot -T svg > dependency-graph.svg",
    "list-bundled-files": "npx depcruise src/react-admin --include-only \"^src\" --output-type text",
    "analyse": "source-map-explorer 'dist/index.mjs' --no-border-checks",
    "bump": "bump-package-versions --part=patch --strategy=highest ../package.json ./package.json ../api/package.json"
  },
  "peerDependencies": {
    "@hookform/resolvers": "2.9.11",
    "@meemoo/react-components": "4.3.2",
    "@studiohyperdrive/pagination": "1.0.0",

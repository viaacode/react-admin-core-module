    "build": "exitzero mv src/index-export.ts src/index.ts && rollup -c && mv src/index.ts src/index-export.ts",
    "copy-to-client-hetarchief": "rimraf ../../hetarchief-client/node_modules/@meemoo/admin-core-ui/dist && rimraf ../../hetarchief-client/.next && cp -r dist package.json ../../hetarchief-client/node_modules/@meemoo/admin-core-ui",

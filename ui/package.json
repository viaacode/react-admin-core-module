{
  "name": "@meemoo/admin-core-ui",
  "version": "3.1.8",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "prepare": "cd .. && husky install",
    "install-local": "npm i ../../react-components ../../avo2-components",
    "dev": "cross-env PORT=3400 dotenv -e .env/.env.local vite",
    "prebuild": "rimraf dist",
    "build": "vite build",
    "serve": "vite preview",
    "copy-to-client-hetarchief": "rimraf ../../hetarchief-client/node_modules/@meemoo/admin-core-ui/dist && rimraf ../../hetarchief-client/.next && cp -r dist package.json ../../hetarchief-client/node_modules/@meemoo/admin-core-ui",
    "copy-to-client-avo": "rimraf ../../avo2-client/node_modules/@meemoo/admin-core-ui/dist ../../avo2-client/node_modules/.cache && cp -r dist package.json ../../avo2-client/node_modules/@meemoo/admin-core-ui",
    "copy": "vite build && npm run copy-to-client-avo && npm run copy-to-client-hetarchief",
    "build:analyze": "npm run build -- --config-analyze",
    "format:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"!src/react-admin/generated/**/*.ts\"",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint --allow-empty-input src/**/*.{css,scss}",
    "lint:scss:fix": "stylelint --fix --allow-empty-input src/**/*.{css,scss}",
    "test": "jest --cache",
    "type-check": "tsc --noEmit",
    "translations:extract": "dotenv -e .env/.env.local ts-node --transpile-only --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.ts && npm run format:fix",
    "bump": "bump-package-versions --part=patch --strategy=highest ../package.json ./package.json ../api/package.json",
    "list-of-changed-translations-hetarchief": "git diff 296b12bc26a4d5c83b8bb35be5f05115ac6356fe b56c8382a7622a13a7f983c682c1094daa514c94 --no-ext-diff --unified=0 --exit-code -a --no-prefix src/shared/translations/hetArchief/nl.json > translation-changes.txt",
    "list-of-changed-translations-avo": "git diff 296b12bc26a4d5c83b8bb35be5f05115ac6356fe b56c8382a7622a13a7f983c682c1094daa514c94 --no-ext-diff --unified=0 --exit-code -a --no-prefix src/shared/translations/avo/nl.json > translation-changes.txt",
    "check-bundled-files": "npx depcruise src/react-admin --config bundle-import-rules.json --include-only \"^src\" --exclude \"(.spec.tsx?|.test.tsx?|node_modules)\"",
    "list-bundled-files": "npx depcruise src/react-admin --include-only \"^src\" --output-type text",
    "analyse": "source-map-explorer 'dist/index.mjs' --no-border-checks"
  },

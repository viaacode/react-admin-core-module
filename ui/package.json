    "format:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"!src/react-admin/generated/**/*.ts\"",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint --allow-empty-input src/**/*.{css,scss}",

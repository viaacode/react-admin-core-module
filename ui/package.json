    "test": "jest --cache",
    "type-check": "tsc --noEmit",
    "translations:extract": "dotenv -e .env/.env.local ts-node --transpile-only --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.ts && npm run format:fix",

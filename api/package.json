{
  "name": "@meemoo/admin-core-api",
  "version": "2.3.19",
  "description": "The backend routes to support the screens in the Admin Core dashboard",
  "author": "Bert Verhelst <bert.verhelst@studiohyperdrive.be>",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy-to-proxy-hetarchief": "rimraf ../../hetarchief-proxy/node_modules/@meemoo/admin-core-api/dist ../../hetarchief-proxy/node_modules/.cache && cp -r dist package.json ../../hetarchief-proxy/node_modules/@meemoo/admin-core-api",
    "copy-to-proxy-avo": "rimraf ../../avo2-proxy/server/node_modules/@meemoo/admin-core-api/dist ../../avo2-proxy/server/node_modules/.cache && cp -r dist package.json ../../avo2-proxy/server/node_modules/@meemoo/admin-core-api",
    "copy": "npm run build && npm run copy-to-proxy-hetarchief && npm run copy-to-proxy-avo",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env/.env.local nest start",
    "dev": "dotenv -e .env/.env.local nest start -- --watch",
    "start:debug": "dotenv -e .env/.env.local nest start -- --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --cache",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.graphql\" \"!src/generated/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "i18n:extract": "ts-node --transpile-only --require dotenv/config scripts/extract-and-replace-translations.ts dotenv_config_path=.env/.env.local && npm run format:fix",
    "generate-hetarchief-database-types": "dotenv -e .env/.env.local graphql-codegen -- --config codegen-hetarchief.yml",
    "generate-avo-database-types": "dotenv -e .env/.env.local graphql-codegen -- --config codegen-avo.yml",
    "generate-database-types": "npm run generate-hetarchief-database-types && npm run generate-avo-database-types",
    "bump": "bump-package-versions --part=patch --strategy=highest ../package.json ../ui/package.json ./package.json",
    "find-circular-dependencies": "npx madge --circular --extensions ts ./"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^7.1.1",
    "@studiohyperdrive/pagination": "1.0.0",
    "@viaa/avo2-types": "2.48.40",
    "app-root-path": "3.0.0",
    "aws-sdk": "2.1097.0",
    "blend-promise-utils": "^1.28.0",
    "cache-manager": "5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.0.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.0",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "got": "^11.8.3",
    "graphql": "16.6.0",
    "helmet": "^5.0.2",
    "i18next": "^21.6.3",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "nock": "^13.2.9",
    "node-cron": "^3.0.0",
    "multer": "^1.4.4",
    "query-string": "^7.1.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "saml2-js": "^3.0.1",
    "session-file-store": "^1.5.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/typed-document-node": "^2.3.12",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.4.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "bump-package-versions": "^1.0.6",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.0",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "typescript": "^5.2.2",
    "graphql": "16.6.0"
  }
}

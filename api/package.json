{
  "name": "@meemoo/admin-core-api",
  "version": "2.0.7",
  "description": "The backend routes to support the screens in the Admin Core dashboard",
  "author": "Bert Verhelst <bert.verhelst@studiohyperdrive.be>",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy-to-proxy-hetarchief": "rimraf ../../hetarchief-proxy/node_modules/@meemoo/admin-core-api/dist && cp -r dist package.json ../../hetarchief-proxy/node_modules/@meemoo/admin-core-api",
    "copy-to-proxy-avo": "rimraf ../../avo2-proxy/server/node_modules/@meemoo/admin-core-api/dist && cp -r dist package.json ../../avo2-proxy/server/node_modules/@meemoo/admin-core-api",
    "build-and-copy-to-avo-and-hetarchief": "npm run build && npm run copy-to-proxy-hetarchief && npm run copy-to-proxy-avo",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env/.env.local nest start",
    "start:dev": "dotenv -e .env/.env.local nest start --watch",
    "start:debug": "dotenv -e .env/.env.local nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "i18n:extract": "ts-node --transpile-only --require dotenv/config scripts/extract-and-replace-translations.ts dotenv_config_path=.env/.env.local && npm run format:fix",
    "generate-hetarchief-database-types": "dotenv -e .env/.env.local graphql-codegen -- --config codegen-hetarchief.yml",
    "generate-avo-database-types": "dotenv -e .env/.env.local graphql-codegen -- --config codegen-avo.yml",
    "generate-database-types": "npm run generate-hetarchief-database-types && npm run generate-avo-database-types"
  },
  "dependencies": {
    "nock": "^13.2.9",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@studiohyperdrive/pagination": "1.0.0",
    "@viaa/avo2-types": "2.36.1",
    "app-root-path": "3.0.0",
    "aws-sdk": "2.1097.0",
    "blend-promise-utils": "^1.28.0",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.0.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.0",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "got": "^11.8.3",
    "graphql": "^15.8.0",
    "helmet": "^5.0.2",
    "i18next": "^21.6.3",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "query-string": "^7.1.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "saml2-js": "^3.0.1",
    "session-file-store": "^1.5.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/joi": "^17.2.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.0",
    "swagger-ui-express": "^4.5.0",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

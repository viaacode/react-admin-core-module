{
  "name": "@meemoo/react-admin",
  "version": "0.7.7",
  "description": "React admin app for meemoo projects",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viaacode/react-admin-core-module.git"
  },
  "keywords": [
    "react",
    "admin"
  ],
  "author": "Andry Charlier <andry.charlier@studiohyperdrive.be>",
  "contributors": [
    {
      "name": "Bert verhelst",
      "email": "bert.verhelst@studiohyperdrive.be"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viaacode/react-admin-core-module/issues"
  },
  "homepage": "https://github.com/viaacode/react-admin-core-module#readme",
  "scripts": {
    "preinstall": "npx force-resolutions",
    "prepare----disabled-since-it-is-throwing-errors": "husky install",
    "start": "cd demo && npm run demo:start",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build-and-copy-to-client": "rimraf ../hetarchief-client/node_modules/@meemoo/react-admin/dist && npm run build && cp -r dist package.json ../hetarchief-client/node_modules/@meemoo/react-admin",
    "build:analyze": "npm run build -- --config-analyze",
    "format": "npm run lint:ts -- --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint demo --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint --allow-empty-input src/**/*.{css,scss}",
    "lint:scss:fix": "stylelint --fix --allow-empty-input src/**/*.{css,scss}",
    "test": "jest --cache",
    "type-check": "tsc --noEmit",
    "generate-database-types": "npm run generate-hetarchief-database-types && npm run generate-avo-database-types",
    "generate-hetarchief-database-types": "dotenv -e .env.local graphql-codegen -- --config codegen-hetarchief.yml",
    "generate-avo-database-types": "dotenv -e .env.local graphql-codegen -- --config codegen-avo.yml",
    "whitelist-queries": "ts-node --project ./scripts/tsconfig.json -- scripts/whitelist-gql-queries.ts",
    "i18n:extract": "ts-node --transpile-only --project ./scripts/tsconfig.json scripts/extract-and-replace-translations.ts && npm run lint:ts -- --fix"
  },
  "peerDependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@meemoo/react-components": "2.15.0",
    "@viaa/avo2-components": "^2.0.4",
    "@viaa/avo2-types": "2.36.1",
    "capture-stack-trace": "1.0.1",
    "clsx": "1.1.1",
    "copy-to-clipboard": "3.3.1",
    "date-fns": "^2.28.0",
    "file-saver": "2.0.5",
    "immer": "9.0.12",
    "ky": "0.30.0",
    "ky-universal": "0.10.1",
    "lodash-es": "4.17.21",
    "moment": "2.29.2",
    "query-string": "7.1.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.0",
    "react-perfect-scrollbar": "1.5.8",
    "react-query": "^3.34.14",
    "react-range": "1.8.12",
    "react-scrollbars-custom": "4.0.27",
    "react-select": "^5.2.2",
    "react-to-string": "0.1.1",
    "sanitize-html": "2.7.0",
    "use-query-params": "1.2.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typed-document-node": "^2.2.11",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-query": "^3.5.12",
    "@graphql-codegen/typescript-resolvers": "^2.6.4",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@rollup/plugin-commonjs": "21.0.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@types/file-saver": "2.0.5",
    "@types/fs-extra": "9.0.13",
    "@types/glob": "7.2.0",
    "@types/lodash-es": "4.17.6",
    "@types/react": "17.0.39",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-dom": "17.0.11",
    "@types/react-table": "7.7.10",
    "@types/sanitize-html": "2.6.2",
    "@types/sort-object-keys": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "autoprefixer": "10.4.2",
    "dotenv-cli": "5.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "fs-extra": "10.1.0",
    "glob": "8.0.3",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "11.2.6",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.68.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "rollup-plugin-visualizer": "5.5.4",
    "sass": "1.49.9",
    "sort-object-keys": "1.1.3",
    "stylelint": "14.4.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "mini-css-extract-plugin": "2.4.7"
  }
}

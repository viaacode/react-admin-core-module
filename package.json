{
  "name": "@meemoo/react-admin",
  "version": "0.1.0",
  "description": "React admin app for meemoo projects",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viaacode/react-admin-core-module.git"
  },
  "keywords": [
    "react",
    "admin"
  ],
  "author": "Andry Charlier <andry.charlier@studiohyperdrive.be>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viaacode/react-admin-core-module/issues"
  },
  "homepage": "https://github.com/viaacode/react-admin-core-module#readme",
  "scripts": {
    "prepare": "husky install",
    "dev": "npm run build && npm pack && cd demo && npm i ../meemoo-react-admin-0.1.0.tgz && npm start",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build:analyze": "npm run build -- --config-analyze",
    "format": "npm run lint:ts -- --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:scss": "stylelint 'src/**/*.{css,scss}' --allow-empty-input",
    "test": "jest --cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "1.1.1",
    "ramda": "0.28.0",
    "react-router-dom": "5.3.0",
    "react-router-guards": "1.0.2",
    "rxjs": "7.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@types/ramda": "0.27.65",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "10.4.2",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "lint-staged": "12.3.3",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.68.0",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "rollup-plugin-visualizer": "5.5.4",
    "sass": "1.49.9",
    "stylelint": "14.4.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
